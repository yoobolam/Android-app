<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context="com.example.bolam.sayusigan.four_listfragment.two_click_listfragment_listview.ListActivity"
    tools:layout_editor_absoluteY="25dp">


    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="400dp"
        android:layout_height="450dp"
        android:layout_marginEnd="5dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="44dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="5dp"
        android:layout_marginLeft="8dp">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="450dp"
            android:layout_height="400dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="115dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.37"
            app:srcCompat="@drawable/noname07" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="100dp"
            android:layout_height="50dp"

            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="45dp"
            android:layout_marginStart="45dp"
            android:layout_marginTop="53dp"
            android:gravity="center"
            android:text="  장소"
            android:textColor="#000000"
            android:textSize="25sp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.432"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <RelativeLayout
            android:id="@+id/relativeLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/gettime"
                android:layout_alignLeft="@+id/Alarmbutton"
                android:layout_alignStart="@+id/Alarmbutton"
                android:layout_marginBottom="48dp"
                android:layout_marginLeft="58dp"
                android:layout_marginStart="58dp"
                android:gravity="center"
                android:text="대기번호"
                android:textColor="@color/kakaobrowm"
                android:textSize="25sp"
                tools:layout_editor_absoluteX="116dp"
                tools:layout_editor_absoluteY="138dp" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/Alarmbutton"
                android:layout_marginBottom="114dp"
                android:layout_toLeftOf="@+id/getorder"
                android:layout_toStartOf="@+id/getorder"
                android:gravity="center"
                android:text="대기시간"
                android:textColor="@color/kakaobrowm"
                android:textSize="25sp"
                tools:layout_editor_absoluteX="215dp"
                tools:layout_editor_absoluteY="138dp" />

            <TextView
                android:id="@+id/getorder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/textView15"
                android:layout_alignBottom="@+id/textView15"
                android:layout_marginLeft="83dp"
                android:layout_marginStart="83dp"
                android:layout_toEndOf="@+id/textView15"
                android:layout_toLeftOf="@+id/textView13"
                android:layout_toRightOf="@+id/textView15"
                android:layout_toStartOf="@+id/textView13"
                android:text="번호"
                android:textAlignment="center"
                android:textColor="@android:color/holo_blue_light"
                android:textColorLink="@android:color/background_dark"
                android:textSize="30dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/gettime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/textView16"
                android:layout_alignLeft="@+id/getorder"
                android:layout_alignStart="@+id/getorder"
                android:layout_toLeftOf="@+id/textView23"
                android:layout_toStartOf="@+id/textView23"
                android:text="시간"
                android:textAlignment="center"
                android:textColor="@android:color/holo_green_light"
                android:textSize="30dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/getorder"
                android:layout_alignBottom="@+id/getorder"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="72dp"
                android:layout_marginRight="72dp"
                android:text="번"
                android:textColor="#000000"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/gettime"
                android:layout_alignBottom="@+id/gettime"
                android:layout_alignLeft="@+id/textView13"
                android:layout_alignStart="@+id/textView13"
                android:text="분"
                android:textColor="#000000"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/Alarmbutton"
                android:layout_width="368dp"
                android:layout_height="wrap_content"

                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="38dp"
                android:text="알림 설정 하러 가기 >"
                android:textAlignment="center"
                android:textColor="@color/colorAccent"
                android:textSize="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/relativeLayout2"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/getstore"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginEnd="46dp"
                android:layout_marginRight="46dp"
                android:layout_marginTop="41dp"
                android:layout_toLeftOf="@+id/getwork"
                android:layout_toStartOf="@+id/getwork"
                android:text="가게"
                android:textAlignment="center"
                android:textColor="@android:color/holo_purple"
                android:textSize="22sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/getwork"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/getstore"
                android:layout_alignBottom="@+id/getstore"
                android:layout_alignEnd="@+id/getorder"
                android:layout_alignRight="@+id/getorder"
                android:layout_marginEnd="44dp"
                android:layout_marginRight="44dp"
                android:text="업무"
                android:textAlignment="center"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="20sp"
                android:textStyle="bold" />

        </RelativeLayout>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/getwork"
            android:layout_alignBottom="@+id/getwork"
            android:layout_alignLeft="@+id/textView17"
            android:layout_alignStart="@+id/textView17"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            android:text="업무"
            android:textColor="#000000"
            android:textSize="25sp"
            android:visibility="invisible" />

    </RelativeLayout>


</android.support.constraint.ConstraintLayout>
