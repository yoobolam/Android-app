<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/viplist_item"
android:layout_width="match_parent"
android:layout_height="150dp"
android:orientation="vertical"
  >

<ImageView
    android:id="@+id/imageView3"
    android:layout_width="match_parent"
    android:layout_height="150dp"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    app:srcCompat="@drawable/noname01" />

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:layout_alignParentBottom="true"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    android:orientation="horizontal"
    android:divider="#ffffff">

    <android.support.constraint.ConstraintLayout
        android:layout_width="93dp"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/vipstore"
            android:layout_width="90dp"
            android:layout_height="25dp"
            android:layout_gravity="start"
            android:layout_marginBottom="15dp"
            android:layout_marginStart="14dp"
            android:layout_marginTop="8dp"
            android:paddingTop="5dp"
            android:text="장소"
            android:textColor="@android:color/holo_purple"
            android:textSize="13sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.539" />

    </android.support.constraint.ConstraintLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="5dp"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp">

                <TextView
                    android:id="@+id/vipwork1"
                    android:layout_width="48dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="15dp"
                    android:paddingTop="5dp"
                    android:text="업무1"
                    android:textAlignment="center"
                    android:textColor="@android:color/holo_orange_dark" />

                <TextView
                    android:id="@+id/TextView21"
                    android:layout_width="55dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/vipwork1"
                    android:layout_alignBottom="@+id/vipwork1"
                    android:layout_marginLeft="12dp"
                    android:layout_marginStart="12dp"
                    android:layout_toEndOf="@+id/vipwork1"
                    android:layout_toRightOf="@+id/vipwork1"
                    android:text="대기시간"
                    android:textAlignment="center"
                    android:textColor="@android:color/background_dark" />

                <TextView
                    android:id="@+id/vipwork2"
                    android:layout_width="48dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:paddingTop="5dp"
                    android:text="업무2"
                    android:textAlignment="center"
                    android:textColor="@android:color/holo_green_light"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/TextView22"
                    android:layout_width="55dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/vipwork2"
                    android:layout_alignBottom="@+id/vipwork2"
                    android:layout_alignLeft="@+id/TextView21"
                    android:layout_alignStart="@+id/TextView21"
                    android:text="대기시간"
                    android:textAlignment="center"
                    android:textColor="@android:color/background_dark" />

                <TextView
                    android:id="@+id/vipwork3"
                    android:layout_width="48dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="14dp"
                    android:paddingTop="5dp"
                    android:text="업무3"
                    android:textAlignment="center"
                    android:textColor="@android:color/holo_blue_dark" />

                <TextView
                    android:id="@+id/TextView23"
                    android:layout_width="55dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/vipwork3"
                    android:layout_alignBottom="@+id/vipwork3"
                    android:layout_alignLeft="@+id/TextView22"
                    android:layout_alignStart="@+id/TextView22"
                    android:paddingTop="5dp"
                    android:text="대기시간"
                    android:textAlignment="center"
                    android:textColor="@android:color/background_dark" />

                <TextView
                    android:id="@+id/viptime1"
                    android:layout_width="48dp"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/vipwork1"
                    android:layout_toEndOf="@+id/TextView21"
                    android:layout_toRightOf="@+id/TextView21"
                    android:text="1"
                    android:textAlignment="center"
                    android:textColor="@android:color/holo_orange_light"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/viptime2"
                    android:layout_width="48dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/TextView22"
                    android:layout_toRightOf="@+id/TextView22"
                    android:text="2"
                    android:textAlignment="center"
                    android:textColor="@android:color/holo_green_light"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/viptime3"
                    android:layout_width="48dp"
                    android:layout_height="wrap_content"

                    android:layout_alignBaseline="@+id/TextView23"
                    android:layout_alignBottom="@+id/TextView23"
                    android:layout_toEndOf="@+id/TextView23"
                    android:layout_toRightOf="@+id/TextView23"
                    android:text="3"
                    android:textAlignment="center"
                    android:textColor="@android:color/holo_blue_light"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/TextView21"
                    android:layout_alignBottom="@+id/TextView21"
                    android:layout_toEndOf="@+id/viptime1"
                    android:layout_toRightOf="@+id/viptime1"
                    android:text="분"
                    android:textColor="@color/common_google_signin_btn_text_dark_focused"
                    tools:layout_editor_absoluteX="43dp"
                    tools:layout_editor_absoluteY="16dp" />

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/viptime2"
                    android:layout_toEndOf="@+id/viptime2"
                    android:layout_toRightOf="@+id/viptime2"
                    android:text="분"
                    android:textColor="@color/common_google_signin_btn_text_dark_focused"
                    tools:layout_editor_absoluteX="43dp"
                    tools:layout_editor_absoluteY="16dp" />

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/viptime3"
                    android:layout_alignBottom="@+id/viptime3"
                    android:layout_toEndOf="@+id/viptime3"
                    android:layout_toRightOf="@+id/viptime3"
                    android:text="분"
                    android:textColor="@color/common_google_signin_btn_text_dark_focused"
                    tools:layout_editor_absoluteX="43dp"
                    tools:layout_editor_absoluteY="16dp" />

            </RelativeLayout>

        </LinearLayout>

    </LinearLayout>

    <android.support.constraint.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent">


        <Button
            android:id="@+id/work1"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/frame"
            android:text="예약"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.08" />

        <Button
            android:id="@+id/work2"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_marginBottom="11dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/frame"
            android:text="예약"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.893"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/work3"
            android:layout_width="50dp"
            android:layout_height="35dp"
            android:layout_marginBottom="3dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="1dp"
            android:background="@drawable/frame"
            android:text="예약"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.875" />
    </android.support.constraint.ConstraintLayout>
</LinearLayout>

</RelativeLayout>